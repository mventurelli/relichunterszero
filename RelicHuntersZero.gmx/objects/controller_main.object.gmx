        }
        if (sprite_exists(weaponBackSprite)) draw_sprite_ext(weaponBackSprite, 2, weaponBackX, weaponBackY+stupidOffsetBackY, 2, 2, 0, weaponBackColor, 0.8*main_hud_alpha);
        
        
