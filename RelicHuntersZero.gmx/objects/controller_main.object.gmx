        exit;
    }
    
    if (room == level1_3) { room_goto(room_shop); exit; }
    if (room == level2_3) { room_goto(room_shop); exit; }
    if (room == level3_3) { room_goto(room_shop); exit; }
    if (room == level4_3) { room_goto(room_shop); exit; }
    
