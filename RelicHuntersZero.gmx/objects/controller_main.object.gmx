            <string>///Save Data and Advance the Game

if (global.stage_current == 1) &amp;&amp; (global.checkpoint1 == 0) global.checkpoint1 = 1;
if (global.stage_current == 2) &amp;&amp; (global.checkpoint2 == 0) global.checkpoint2 = 1;
