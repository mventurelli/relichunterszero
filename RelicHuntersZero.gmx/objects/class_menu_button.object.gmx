        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Toggle Button Draw Hack

if (instance_exists(myMinusButton)) &amp;&amp; (instance_exists(myPlusButton))
{
    if (myMinusButton.isSelected) isSelected = true;
    if (myPlusButton.isSelected) isSelected = true;
}
</string>
