        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Main Stuff

isAvailable = true;
labelString = 'MISSING LABEL';
allowConsoleSelection = true;
myFont = global.font_menuMedium;

myVariable = -99;
myVariableMax = -99;
parentButton = noone;
myMinusButton = noone;
myPlusButton = noone;

image_speed = 0;

isSelected = false;
allowMultipleSelection = false;
selectionOrderVertical = 0;
