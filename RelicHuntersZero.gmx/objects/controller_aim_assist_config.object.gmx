            <string>///Control the Menu
goToNextRoom = false;

if (global.input == 'joystick')
{
    if (joy_simple_deadzone( joy_xpos(0), joy_ypos(0), 0.3))
    {
        if (allowSwitch)
        {
            var joyDirection = point_direction(x,y,(x+joy_xpos(0)),(y+joy_ypos(0)));
            
            if (joyDirection &lt;= 45) || (joyDirection &gt;= 315) aimMode = 1;
            else aimMode = 0;
            
            allowSwitch = false;
        }
    }
    else allowSwitch = true;

    if (joy_check_pressed(0,1)) goToNextRoom = true;
}
else goToNextRoom = true;

if keyboard_check(vk_escape) goToNextRoom = true;

if (goToNextRoom)
{
    if (aimMode) global.auto_aim = K_AUTOAIM_ASSIST;
    else global.auto_aim = K_AUTOAIM_FULL;
    
    ini_open(working_directory + "\saveData.ini");
    ini_write_real('Input','Auto Aim',global.auto_aim);
    ini_close();
    
    room_goto_next();
}
